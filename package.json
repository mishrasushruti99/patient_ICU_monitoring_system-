    "@fullcalendar/core": "^4.3.1",
    "@fullcalendar/daygrid": "^4.3.0",
    "@fullcalendar/interaction": "^4.3.0",
    "@fullcalendar/list": "^4.3.0",
    "@fullcalendar/timegrid": "^4.3.0",
    "@fullcalendar/vue": "^4.3.1",
    "animate.css": "^3.7.2",
    "apexcharts": "^3.8.6",
    "axios": "0.18.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.3",
    "c3": "^0.7.11",
    "chart.js": "^2.8.0",
    "chartist": "^0.11.4",
    "lodash": "4.17.11",
    "metismenujs": "1.0.3",
    "v-click-outside": "^2.1.4",
    "v-mask": "^2.0.2",
    "vue": "^2.6.10",
    "vue-apexcharts": "^1.5.0",
    "vue-c3": "^1.2.11",
    "vue-chartist": "^2.2.1",
    "vue-chartjs": "^3.4.2",
    "vue-count-to": "^1.0.13",
    "vue-draggable": "^2.0.2",
    "vue-form-wizard": "^0.8.4",
    "vue-knob-control": "^1.6.0",
    "vue-meta": "1.6.0",
    "vue-multiselect": "^2.1.6",
    "vue-nestable": "^2.4.5",
    "vue-perfect-scrollbar": "^0.2.0",
    "vue-router": "3.0.6",
    "vue-slide-bar": "^1.2.0",
    "vue-string-filter": "^1.3.0",
    "vue-sweetalert2": "^2.1.1",
    "vue-switches": "^2.0.1",
    "vue-tour": "^1.1.0",
    "vue2-animate": "^2.1.2",
    "vue2-datepicker": "^2.12.0",
    "vue2-dropzone": "^3.6.0",
    "vue2-editor": "^2.10.2",
    "vue2-google-maps": "^0.10.7",
    "vuelidate": "^0.7.4",
    "vuetable-2": "^1.7.5",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.7.x",
    "@vue/cli-plugin-e2e-cypress": "3.7.x",
    "@vue/cli-plugin-eslint": "3.7.x",
    "@vue/cli-plugin-unit-jest": "3.7.x",
    "@vue/cli-service": "3.7.x",
    "@vue/eslint-config-prettier": "4.0.x",
    "@vue/eslint-config-standard": "4.0.x",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.x",
    "babel-jest": "23.6.x",
    "cross-env": "5.2.x",
    "eslint": "5.16.x",
    "eslint-plugin-vue": "vuejs/eslint-plugin-vue#edd5f14",
    "express": "4.16.x",
    "hygen": "2.1.2",
    "imagemin-lint-staged": "0.4.x",
    "lint-staged": "8.1.x",
    "markdownlint-cli": "0.15.x",
    "npm-run-all": "4.1.x",
    "sass": "1.19.x",
    "sass-loader": "7.1.x",
    "stylelint": "10.0.x",
    "stylelint-config-css-modules": "1.4.x",
    "stylelint-config-prettier": "5.0.x",
    "stylelint-config-recess-order": "2.0.x",
    "stylelint-config-standard": "18.3.x",
    "stylelint-scss": "3.6.x",
    "vue-template-compiler": "2.6.10",
    "vuepress": "0.14.x"
  },
  "engines": {
    "node": ">=10.13.3",
    "yarn": ">=1.0.0"
  }
}
